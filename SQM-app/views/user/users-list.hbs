
<div class="container-fluid" id="main-table">
  <div id="toolbar">
  </div>
  
  <table id="table" class="table table-hover table-dark">
    <thead>
      <tr>
        <th scope="col"></th>
        <th scope="col">Organization</th>
        <th scope="col">Username</th>
        <th scope="col">Email</th>
      </tr>
    </thead>
    <tbody>
      {{#each usersList}}
        <tr>
          <td><a class="btn btn-outline-warning" href="/auth/edit-user/{{_id}}">Edit</a></td>
          <td>{{org}}</td>
          <td>{{username}}</td>
          <td>{{email}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
